#!/bin/bash

providersOld="uninett telekom netcom telenor"
providersNew="uninett hafslund dfn"

# ###### Check two lists of providers and return providers removed ##########
getRemovedProviders()
{
   local oldProviders="$1"
   local newProviders="$2"

   local removedProviders=""
   for o in $oldProviders ; do
      found=0
      for n in $newProviders ; do
         if [ "$o" = "$n" ] ; then
            found=1
            break
         fi
      done
      if [ $found -eq 0 ] ; then
         if [ "$removedProviders" = "" ] ; then
            removedProviders="$o"
         else
            removedProviders="$removedProviders $o"
         fi
      fi
   done
   echo "$removedProviders"
}



getRemovedProviders "$providersOld" "$providersNew"
