;
; BIND data file for local loopback interface
;

$TTL	604800

@	IN	SOA	svalbard.nornet. root.svalbard.nornet. (
			      2		; Serial
			   360		; Refresh
			    60		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL

@	IN	NS	tunnelbox.svalbard.nornet.
@	IN 	MX	10 tunnelbox.svalbard.nornet.


tunnelbox	IN	A	10.99.99.1
tunnelbox	IN	AAAA	fd99::1
tunnelbox	IN 	HINFO   "The Tunnelbox at Svalbard" "Linux"
tunnelbox	IN	LOC	79 1 2 N 10 1 1 E 5m 25m

node2	IN A	10.99.99.2
node3	IN A	10.99.99.3
node4	IN A	10.99.99.4
node5	IN A	10.99.99.5
node6	IN A	10.99.99.6
node7	IN A	10.99.99.7
node8	IN A	10.99.99.8

node2	IN	AAAA	fd99::2
node3	IN	AAAA	fd99::3
node4	IN	AAAA	fd99::4
node5	IN	AAAA	fd99::5
node6	IN	AAAA	fd99::6
node7	IN	AAAA	fd99::7
node8	IN	AAAA	fd99::8


www	IN	CNAME	tunnelbox.svalbard.nornet.
ntp	IN	CNAME	tunnelbox.svalbard.nornet.
ns1	IN	CNAME	tunnelbox.svalbard.nornet.
